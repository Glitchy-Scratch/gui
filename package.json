    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.1",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^4.5.2",
    "jest": "^21.0.0",
    "jest-junit": "^12.2.0",
    "mkdirp": "^1.0.4",
    "raf": "^3.4.0",
    "react-test-renderer": "16.2.0",
